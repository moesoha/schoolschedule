<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="dev.soha.course202002.schedule.android"
	>

	<uses-permission android:name="android.permission.INTERNET" />

	<application
		android:name="dev.soha.course202002.schedule.android.Application"
		android:allowBackup="true"
		android:fullBackupContent="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/Theme.SchoolSchedule"
		>
		<activity android:name="dev.soha.course202002.schedule.android.ui.lesson.LessonShowActivity">
		</activity>
		<activity
			android:name="dev.soha.course202002.schedule.android.ui.oalogin.OaLoginActivity"
			android:label="@string/title_activity_oa_login"
			>
		</activity>
		<activity
			android:name="dev.soha.course202002.schedule.android.ui.MainActivity"
			android:label="@string/app_name"
			>
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	</application>

</manifest>